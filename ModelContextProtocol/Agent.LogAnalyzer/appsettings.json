{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AgentConfiguration": {
    "Name": "LogAnalyzerAgent",
    "Description": "Analyzes logs for errors and warnings.",
    "Tools": ["analyze"],
    "OrchestratorUrl": "http://localhost:5131/register",
    "SanitizerUrl": "http://localhost:5197/sanitize",
    "Endpoint": "http://localhost:5074"
  },
  "LogAnalysis": {
    "Patterns": {
      "Warning": "warning",
      "Error": "error",
      "Critical": "critical"
    }
  }
}
